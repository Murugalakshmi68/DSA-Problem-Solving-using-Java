class Solution {
    public ArrayList<Integer> getMinMax(int[] arr) {
        // code Here
    int min=arr[0];
    int max=arr[0];
    for ( int i : arr ){
        if(min > i)
            min = i;
        if(max < i)
            max = i;
    }
    ArrayList<Integer> result = new ArrayList<>();
    result.add(min);
    result.add(max);

    return result;
    
    }
}
